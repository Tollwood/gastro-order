{
  "name": "gastro-order",
  "version": "1.0.0",
  "main": "build/index",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist build client/build",
    "install": "yarn --cwd client install",
    "build": "tsc && yarn build:client",
    "copy:client": "cp -r client/build build/public",
    "build:client": "yarn --cwd client build && yarn copy:client",
    "dev": "nodemon",
    "start": "node build/server.js",
    "dist": "sh ./scripts/dist.sh",
    "deploy": "eb deploy",
    "full-deploy": "yarn clean && yarn build && yarn dist && yarn deploy"
  },
  "dependencies": {
    "aws-sdk": "^2.715.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.6.0",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.4",
    "mongoose": "^5.10.14",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.8",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.9",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/jwk-to-pem": "^2.0.0",
    "@types/mongoose": "^5.10.1",
    "@types/node": "^14.14.7",
    "@types/node-fetch": "^2.5.7",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  }
}
